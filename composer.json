{
    "name": "ruwork/project",
    "description": "A basic Ruwork project.",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.8.1",
        "doctrine/doctrine-migrations-bundle": "^1.3.1",
        "doctrine/orm": "^2.6.1",
        "fsi/ckeditor-bundle": "^7.0",
        "gregwar/captcha-bundle": "^2.0",
        "guzzlehttp/psr7": "^1.4",
        "hostnet/form-handler-bundle": "^1.6",
        "php-http/curl-client": "^1.7",
        "php-http/httplug-bundle": "^1.7",
        "ruvents/admin-bundle": "^0.7.16",
        "ruvents/doctrine-fixes-bundle": "^0.1.1",
        "ruvents/twig-extensions": "^3.2",
        "ruwork/doctrine-behaviors-bundle": "^0.2",
        "ruwork/feature-bundle": "^0.1.0",
        "ruwork/filter-bundle": "^0.1",
        "ruwork/frujax-bundle": "^0.2",
        "ruwork/manual-auth-bundle": "^0.1.2",
        "ruwork/reform-bundle": "^0.1",
        "ruwork/routing-tools-bundle": "^0.1",
        "ruwork/ruwork-bundle": "^0.1.4",
        "ruwork/synchronizer-bundle": "^0.1.0",
        "ruwork/template-i18n-bundle": "^0.2.0",
        "ruwork/upload-bundle": "^0.1.3",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.0.6",
        "symfony/cache": "^4.0.6",
        "symfony/console": "^4.0.6",
        "symfony/expression-language": "^4.0.6",
        "symfony/flex": "^1.0.71",
        "symfony/form": "^4.0.6",
        "symfony/framework-bundle": "^4.0.6",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.2.0",
        "symfony/property-access": "^4.0.6",
        "symfony/property-info": "^4.0.6",
        "symfony/security-bundle": "^4.0.6",
        "symfony/serializer": "^4.0.6",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.0.6",
        "symfony/validator": "^4.0.6",
        "symfony/web-link": "^4.0.6",
        "symfony/yaml": "^4.0.6",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.3",
        "hautelook/alice-bundle": "^2.0",
        "symfony/debug-bundle": "^4.0.6",
        "symfony/dotenv": "^4.0.6",
        "symfony/maker-bundle": "^1.2.0",
        "symfony/stopwatch": "^4.0.6",
        "symfony/var-dumper": "^4.0.6",
        "symfony/web-profiler-bundle": "^4.0.6",
        "symfony/web-server-bundle": "^4.0.6"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --release=full --clear=skip": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "touch config/parameters.yaml",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "touch config/parameters.yaml",
            "@auto-scripts"
        ],
        "sync": [
            "@composer install",
            "@php bin/console doctrine:migrations:migrate --allow-no-migration --no-interaction",
            "yarn install",
            "node node_modules/gulp/bin/gulp.js build"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C8T3K9FCNC3FD2G38674Y3FH",
            "allow-contrib": false
        }
    }
}
